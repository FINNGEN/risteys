                    <section>
                        <h3>Data source</h3>
                        <table class="table-fixed">
                            <tbody>
                                <tr>
                                    <td>Registries</td>
                                    <td>Inpatient, KELA</td>
                                </tr>
                                <tr>
                                    <td>ICD-10 codes</td>
                                    <td>X00, Y11, Z11</td>
                                </tr>
                                <tr>
                                    <td>Finngen reference</td>
                                    <td><%= @code %></td>
                                </tr>
                            </tbody>
                        </table>
                    </section>

                    <section id="key_figures">
                        <h3>Key figures</h3>
                        <table class="table-fixed key-figures">
                            <tbody>
                                <tr>
                                    <td></td>
                                    <td>Number of events</td>
                                    <td>Prevalence (%)</td>
                                    <td>Mean age after baseline (years)</td>
                                </tr>
                                <tr>
                                    <td>All</td>
                                    <td><%= @num_cases %></td>
                                    <td><%= prevalence(@num_cases, @num_controls) %></td>
                                    <td>100</td>
                                </tr>
                                <tr>
                                    <td>Male</td>
                                    <td><%= trunc(@num_cases * 0.4) %></td>
                                    <td><%= prevalence(@num_cases, @num_controls) %></td>
                                    <td>100</td>
                                </tr>
                                <tr>
                                    <td>Female</td>
                                    <td><%= trunc(@num_cases * 0.6) %></td>
                                    <td><%= prevalence(@num_cases, @num_controls) %></td>
                                    <td>100</td>
                                </tr>
                            </tbody>
                        </table>
                    </section>
